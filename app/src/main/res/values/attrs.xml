<?xml version="1.0" encoding="utf-8"?>
<resources>
	<attr name="snow_level">
		<enum name="huge"
			value="1"/>
		<enum name="big"
			value="2"/>
		<enum name="middle"
			value="3"/>
		<enum name="small"
			value="4"/>
	</attr>
	<attr name="snow_color"
		format="color"/>

	<attr name="snow_flake_type">
		<enum name="color"
			value="1"/>
		<enum name="bitmap"
			value="2"/>
	</attr>

	<declare-styleable name="SnowView">
		<attr name="snow_level"/>
		<attr name="snow_color"/>
		<attr name="snow_flake_type"/>
	</declare-styleable>
</resources>